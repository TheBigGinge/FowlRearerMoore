    "uglify-js": "3.2.2",
    "uglifyjs-webpack-plugin": "1.1.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  },
  "scripts": {
    "start": "npm run dev",
    "test": "node node_modules/webpack/bin/webpack.js --config webpack.test.config.js && node run-tests.js",
    "tc-test": "node node_modules/webpack/bin/webpack.js --config webpack.test.config.js && node run-tests.js --tc",
    "test-only": "node run-tests.js",
    "test-watch": "nodemon --watch src -q -e js,jsx,ts,tsx -x \"npm test\"",
    "webpack": "node node_modules/webpack/bin/webpack.js",
    "stats": "node node_modules/webpack/bin/webpack.js --json > stats.json",
    "devserver": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "dev": "npm run devserver -- --config webpack.dev.config.js",
    "dev-legacy": "npm run devserver -- --config webpack.dev.config.js --env.includeLegacy=true",
    "dev-cold": "npm run devserver -- --config webpack.dev.cold.config.js",
    "dev-cold-legacy": "npm run devserver -- --config webpack.dev.cold.config.js --env.includeLegacy=true",
    "lint": "npm run eslint && npm run tslint && npm run scsslint",
    "eslint": "node node_modules/eslint/bin/eslint src/**/*.js*",
    "tslint": "node node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json",
    "scsslint": "node node_modules/stylelint/bin/stylelint \"src/**/*.scss\"",
    "tc-eslint": "node node_modules/eslint/bin/eslint --format node_modules/eslint-teamcity/index.js src",
    "tc-tslint": "node node_modules/tslint/bin/tslint --type-check --project tsconfig.json --formatters-dir node_modules/tslint-teamcity-reporter/ --format TSHintTeamcity src/**/*.ts*",
    "tc-scsslint": "node node_modules/stylelint/bin/stylelint \"src/**/*.scss\" --custom-formatter=node_modules/stylelint-teamcity-reporter"
  }
}
